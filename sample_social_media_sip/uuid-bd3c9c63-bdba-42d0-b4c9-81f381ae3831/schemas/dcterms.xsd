   <xsd:element name="relation" substitutionGroup="dc:relation"/>
   <xsd:element name="coverage" substitutionGroup="dc:coverage"/>
   <xsd:element name="rights" substitutionGroup="dc:rights"/>

   <xsd:element name="alternative" substitutionGroup="title"/>

   <xsd:element name="tableOfContents" substitutionGroup="description"/>
   <xsd:element name="abstract" substitutionGroup="description"/>

   <xsd:element name="created" substitutionGroup="date"/>
   <xsd:element name="valid" substitutionGroup="date"/>
   <xsd:element name="available" substitutionGroup="date"/>
   <xsd:element name="issued" substitutionGroup="date"/>
   <xsd:element name="modified" substitutionGroup="date"/>
   <xsd:element name="dateAccepted" substitutionGroup="date"/>
   <xsd:element name="dateCopyrighted" substitutionGroup="date"/>
   <xsd:element name="dateSubmitted" substitutionGroup="date"/>

   <xsd:element name="extent" substitutionGroup="format"/>
   <xsd:element name="medium" substitutionGroup="format"/>

   <xsd:element name="isVersionOf" substitutionGroup="relation"/>
   <xsd:element name="hasVersion" substitutionGroup="relation"/>
   <xsd:element name="isReplacedBy" substitutionGroup="relation"/>
   <xsd:element name="replaces" substitutionGroup="relation"/>
   <xsd:element name="isRequiredBy" substitutionGroup="relation"/>
   <xsd:element name="requires" substitutionGroup="relation"/>
   <xsd:element name="isPartOf" substitutionGroup="relation"/>
   <xsd:element name="hasPart" substitutionGroup="relation"/>
   <xsd:element name="isReferencedBy" substitutionGroup="relation"/>
   <xsd:element name="references" substitutionGroup="relation"/>
   <xsd:element name="isFormatOf" substitutionGroup="relation"/>
   <xsd:element name="hasFormat" substitutionGroup="relation"/>
   <xsd:element name="conformsTo" substitutionGroup="relation"/>

   <xsd:element name="spatial" substitutionGroup="coverage"/>
   <xsd:element name="temporal" substitutionGroup="coverage"/>

   <xsd:element name="audience" substitutionGroup="dc:any"/>
   <xsd:element name="accrualMethod" substitutionGroup="dc:any"/>
   <xsd:element name="accrualPeriodicity" substitutionGroup="dc:any"/>
   <xsd:element name="accrualPolicy" substitutionGroup="dc:any"/>
   <xsd:element name="instructionalMethod" substitutionGroup="dc:any"/>
   <xsd:element name="provenance" substitutionGroup="dc:any"/>
   <xsd:element name="rightsHolder" substitutionGroup="dc:any"/>

   <xsd:element name="mediator" substitutionGroup="audience"/>
   <xsd:element name="educationLevel" substitutionGroup="audience"/>

   <xsd:element name="accessRights" substitutionGroup="rights"/>
   <xsd:element name="license" substitutionGroup="rights"/>

   <xsd:element name="bibliographicCitation" substitutionGroup="identifier"/>

  <xsd:complexType name="LCSH">
   <xsd:simpleContent>
    <xsd:restriction base="dc:SimpleLiteral">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string"/>
        </xsd:simpleType>
        <xsd:attribute ref="xml:lang" use="prohibited"/>
    </xsd:restriction>
   </xsd:simpleContent>
  </xsd:complexType>

