    <xsd:attribute name="CONTENTINFORMATIONTYPE">
        <xsd:simpleType>
            <xsd:restriction base="xsd:string">
                <xsd:enumeration value="ERMS"/>
                <xsd:enumeration value="SIARD1"/>
                <xsd:enumeration value="SIARD2"/>
