                <xsd:enumeration value="citserms_v2_1"/>
                <xsd:enumeration value="citsehpj_v1_0"/>
                <xsd:enumeration value="citsehcr_v1_0"/>
                <xsd:enumeration value="citssiard_v1_0"/>
                <xsd:enumeration value="citsgeospatial_v3_0"/>
